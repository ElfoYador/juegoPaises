<Window x:Class="InterfazJuego.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InterfazJuego"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="ventanaJugadores">
        <TextBox HorizontalAlignment="Left" Height="23" Margin="325,118,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label Uid="LabelJugador" Name="LabelJugador" Content="Jugador:" HorizontalAlignment="Left" Margin="265,118,0,0" VerticalAlignment="Top"/>
        <Label Uid="LabelContinente" Name="LabelContinente" Content="Continente:" HorizontalAlignment="Left" Margin="255,172,0,0" VerticalAlignment="Top"/>
        <Label Name="Titulo" Uid="Titulo" Content="Juego de los Paises" HorizontalAlignment="Left" Margin="297,27,0,0" VerticalAlignment="Top" Height="45" Width="215" FontFamily="Tahoma" FontSize="18" FontWeight="Bold"/>
        <Button Uid="Jugar" Name="Jugar" Content="Jugar" HorizontalAlignment="Left" Margin="370,244,0,0" VerticalAlignment="Top" Width="75"/>
        <ComboBox Uid="cmb_Continentes" Name="cmb_Continentes" HorizontalAlignment="Left" Margin="325,172,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
